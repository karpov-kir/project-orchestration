{
  "compilerOptions": {
    "types": [
      "vinxi/types/client",
      "@solidjs/start/env",
      "vite-plugin-solid-svg/types",
    ],
    "noEmit": true,
    "strict": true,
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
